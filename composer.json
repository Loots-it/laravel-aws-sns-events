{
    "name": "loots-it/laravel-sns",
    "description": "A simple library to validate aws sns webhook calls and map these calls to Laravel events.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LootsIt\\LaravelSns\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "aws/aws-php-sns-message-validator": "^1.8",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LootsIt\\LaravelSns\\Providers\\AwsSnsServiceProvider"
            ]
        }
    }
}
